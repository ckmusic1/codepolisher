{
	"version": "0.2.0",
	"configurations": [
	  {
		"name": "Run Extension",
		"type": "extensionHost",
		"request": "launch",
		"args": [
		  "--extensionDevelopmentPath=${workspaceFolder}"
		],
		"outFiles": [
		  "${workspaceFolder}/out/**/*.js"
		],
		"preLaunchTask": "npm: watch"
	  },
	  {
		"name": "Dart",
		"type": "dart",
		"request": "launch",
		"program": "bin/main.dart"
	  }
	]
  }
  